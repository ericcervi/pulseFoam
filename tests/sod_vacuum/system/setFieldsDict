FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U               (0 0 0)
    volScalarFieldValue T               289672
    volScalarFieldValue p               5e4
    volScalarFieldValue rho_m0          6e-4
    volScalarFieldValue alpha_m0        1
);

regions
(
    boxToCell
    {
        box (0.5  -1 -1) (10  1 1);
        fieldValues
        (
            volScalarFieldValue T           1
            volScalarFieldValue p           1e-16
            volScalarFieldValue rho_m0      3.49e-19
        );
    }
    
);


