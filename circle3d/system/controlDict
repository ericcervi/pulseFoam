FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoPimpleFoam;

Tmax -0.003483;

fluxScheme 1;

energyGroups    1;

externalCycles  1;

eulRel  1;

materials  2;

quasi_single false;

criticalityCalculation 0;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.03;

deltaT          1.25e-7; //start with 1.25e-7 --> 0.5e-7 upon gas jet impact 

writeControl    runTime;

writeInterval   1e-4;

purgeWrite      0;

writeFormat     ascii;

writePrecision  15;

nominalPower 100;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

maxCo           0.5;

KeffControl     0.97063;



